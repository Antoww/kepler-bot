{
    "name": "kepler-bot",
    "watcher": {
        "interval": 1000,
        "exts": ["js"],
        "skip": ["json","**/.git/**"]
    },

    "processes": [
        {
            "id": "kepler-bot",
            "cmd" : "deno run --allow-all index.js",
            "autostart": true
        }
    ]
}